{
  "name": "Energy Ingestion Dataflow",
  "description": "Loads raw CSVs for energy production, district heating, CO2 emissions, and DimDate into Fabric Lakehouse.",
  "entities": [
    {
      "name": "EnergyProduction",
      "source": "File",
      "path": "project1-energy-bi/data/energy_production.csv",
      "output": "Lakehouse.Raw.FactEnergyProduction"
    },
    {
      "name": "DistrictHeating",
      "source": "File",
      "path": "project1-energy-bi/data/district_heating.csv",
      "output": "Lakehouse.Raw.FactDistrictHeating"
    },
    {
      "name": "CO2Emissions",
      "source": "File",
      "path": "project1-energy-bi/data/co2_emissions.csv",
      "output": "Lakehouse.Raw.FactCO2Emissions"
    },
    {
      "name": "DimDate",
      "source": "File",
      "path": "project1-energy-bi/data/dim_date.csv",
      "output": "Lakehouse.Raw.DimDate"
    }
  ]
}
